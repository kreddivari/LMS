<div class="container-fluid">
	<div class="row  tile">
		<!-- tile -->
		<div class="tile-header bg-greensea dvd dvd-btm">
			<h1 class="custom-font">
				<strong>Dash Board</strong>
			</h1>
		</div>
		
		<div class="col-md-12">

        <!-- tile -->
        <section class="tile" fullscreen="isFullscreen12">

          <!-- tile header -->
          
          <!-- /tile header -->
			
          <!-- tile body -->
          <div class="tile-body" >

            <!-- row -->
            <div class="row" >
              <!-- col -->
              <div class="col-md-6">
				<label><h4>Facebook Sentiment</h4></label>
                <div  morris-line-chart
                      line-data='facebookData'
                      line-xkey='year'
                      line-ykeys='["score"]'
                      line-labels='["Facebook Sentiment"]'
                      line-colors='["#16a085","#FF0066"]' style="height: 250px"></div>
              </div>
             
              <!-- /col -->
               <div class="col-md-6">
               <label><h4>Twitter Sentiment</h4></label>
                <div  morris-line-chart
                      line-data='twitterData'
                      line-xkey='year'
                      line-ykeys='["score"]'
                      line-labels='["Twitter Sentiment"]'
                      line-colors='["#16a085","#FF0066"]' style="height: 250px"></div>
              </div>
              <!-- /col -->
            </div>
            <!-- /row -->
			
          </div>
          <!-- /tile body -->
	
        </section>
        <!-- /tile -->
	</div>
   </div>
</div>













 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 




 