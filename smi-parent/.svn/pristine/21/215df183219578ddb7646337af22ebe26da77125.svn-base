  <div class="btn-group pull-right caseCategoryDesc">
      <div class="row tile">
      <div class=" tile-body">
       <div class="col-md-12">
           <div class="col-md-3 ">
            <label class="checkbox checkbox-custom checkbox-custom-sm m-0 mail-select">
        <input type="checkbox"><i></i> Select All</label>
       <!--   <a  class=" br-2 w-60"><i class="fa fa-long-arrow-left"></i></a>-->
        </div>
        <div class="col-md-4 "> 
         <input id="filter"  class="form-control input-sm w-sm mb-10 inline-block " placeholder="search "> 
         <i class="icon-magnifier"></i>
        </div>
     <div class="btn-group col-md-1 ">
 <button class="btn btn-default btn-sm br-2 text-right"><i class="fa fa-refresh"></i></button>
 </div>
 <div class ="col-md-2">
 <select
	class="form-control" data-ng-model="a.name"
	data-ng-options="a.name for a in moreOptions" ng-change="openDlgAssign()">
	<option value="">More</option>
</select>
</div>
</div>
</div> 
<div class="col-md-12" data-ng-click="individualCaseDetails()"> 
<ul class="list-group no-radius no-border" id="mails-list">
   <li ng-repeat="case in fbCases" class="list-group-item b-{{mail.label.color}}">
 
     <div class="media">
    
       <div class="pull-left">
        <label class="checkbox checkbox-custom checkbox-custom-sm m-0 mail-select">
        <input type="checkbox"><i></i></label>
         <div class="thumb thumb-sm">
           <img src="{{case.image}}" class="img-circle" > 
         </div>
       </div>
       <div class="media-body">
         <div class="media-heading m-0">
           <a class="mr-5">{{case.customerName}}</a>
           <span class="pull-right text-sm text-muted">
             <span class="hidden-xs">{{ case.date }}</span>
           </span>
         </div>
         <small>{{case.content}}</small>
       </div>
       
        <div class="form-group col-md-12 tile-body ">
        <label  class="col-md-4 "><b>Case Priority : {{case.casePriority}}</b></label>
        <label class="col-md-4 "><b>Social Account : {{case.socialAccount}}</b></label>
        <label class="col-md-4 "><b>Case Status :  {{case.caseStatus}}</b></label>
</div>
    </div>
   </li>
 </ul>
</div>
 
</div>

<!-- right side body -->
<div>
</div>
  </div>
  </div>
  <!-- /page content -->
 

   <script type="text/ng-template" id="assignTo.html">
      <div class="modal-header">
      <h3 class="modal-title custom-font">Case Assignment</h3>
      </div>
     <div class="modal-body">
       <table st-table="rowCollection" class="st-table table table-striped table-custom">
         <tbaody>
			
            <label class="col-lg-4 text-right">Assign To</label>
		                <div class="col-lg-4">
		                  <select class="form-control mb-10"  data-ng-model="appId" data-ng-options="rulesApps.appId as rulesApps.appName for rulesApps in appData" data-ng-change="">
		                  <option value="">Select</option>
		                  </select>
		              	</div>
		
             <tr>
                 <td>Resons for Assigning </td>
                <td><textarea class="form-control" rows="2" name="message" id="message" placeholder="Type your message" ng-model="formula"  on-blur-validation required></textarea></td>
            </tr>
         </tbody>
        </table>
      </div>
      <div class="modal-footer">
         <button class="btn btn-success btn-ef btn-ef-3 btn-ef-3c" ng-click="ok()"><i class="fa fa-arrow-right"></i> Save</button>
         <button class="btn btn-lightred btn-ef btn-ef-4 btn-ef-4c" ng-click="cancel()"><i class="fa fa-arrow-left"></i> Cancel</button>
      </div>
     </script>
     
