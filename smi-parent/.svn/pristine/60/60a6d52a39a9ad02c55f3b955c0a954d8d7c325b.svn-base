					<ul>
						<li ui-sref-active="active"><a ui-sref="agents"
							data-ng-click="initializeAgentsControllerView()"><fa
									name="caret-right"></fa>Agents</a></li>
					</ul></li>
			</ul></li>
			
			<li ui-sref-active="active"><a ui-sref="#"> <i class="glyphicon glyphicon-bullhorn"></i>
			<span>Brand Marketing</span></a>
			<ul>
				<li ui-sref-active="active"><a ui-sref="posts">
				                 <fa name="caret-right"></fa>Posts</a></li>
				<li ui-sref-active="active"><a ui-sref="allposts" data-ng-click="initializeAllPostController()"> <fa name="caret-right"></fa>All Posts</a></li>
		</ul>
		</li>
	</ul>

	</accordion-group> </accordion>
