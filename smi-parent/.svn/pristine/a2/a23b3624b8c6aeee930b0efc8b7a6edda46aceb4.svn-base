body {
	font-family: Roboto, sans-serif;
	font-size: 15px;
	color: #000;
	background: #fff
}
.uk-container {
	padding-bottom: 0;
	padding-top: 0;
	margin: 0 auto
}

.uk-height-viewport {
	height: calc(100vh - 250px);
	min-height: 600px
}
.tm-fullscreen-container {
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	overflow: hidden;
	position: relative;
	z-index: 1
}
.tm-fullscreen-container .tm-fullscreen {
	height: 80%;
    width: 100%;
    padding: 0;
    background: url(../images/omni-presence.jpg) 50% 50%/cover no-repeat #8D8D8D;
    /* position: absolute; */
    margin: 0 auto;
    bottom: 0;
}
}
.tm-fullscreen-container .tm-fullscreen.fs2 {
	height: 100%;
	width: 100%;
	padding: 0;
	background: url(../images/omni-presence2.jpg) 50% 50%/cover no-repeat #8D8D8D;
	position: absolute;
	bottom: 0
}
.tm-fullscreen-container .tm-fullscreen.fs3 {
	background: url(../images/omni-presence3.jpg) 50% 50%/cover no-repeat #8D8D8D
}

.tm-fullscreen-container .tm-fullscreen.fs4 {
	background: url(images/gartner-customer360.jpg) 50% 50%/cover no-repeat #8D8D8D
}
.caption {
	position: absolute;
	bottom: 25%;
	line-height: normal;
	width: 100%;
	font-family: Roboto, sans-serif;
	font-size: 15px;
	color: #fff
}
.caption h1 {
	font-family: Roboto, sans-serif;
	font-weight: 300;
	text-shadow: none;
	font-size: 44px;
	color: #fff;
	background: url(../images/underline.png) 50% 100% no-repeat;
	padding-bottom: 20px
}
.uk-button-primary {
	background-color: #337ab7;
	font-family: Arial, Helvetica, sans-serif;
	font-weight: 700;
	text-transform: uppercase;
	padding: 6px 50px;
	font-size: 12px;
	margin-top: 25px;
	width:250px;
	margin-bottom: 20px;
}
.uk-button {
	background: #337ab7;
	text-shadow: none;
	font-family: Arial, Helvetica, sans-serif;
	text-transform: uppercase;
	font-weight: 700;
	color: #fff;
	padding: 5px 20px;
	font-size: 12px
}

.uk-button-dark:active, .uk-button-dark:focus, .uk-button-dark:hover {
	background: rgba(241,241,241,.3);
	border: 1px solid #000;
	color: #000
}
.uk-button-secondary {
	background: #5cb85c;
	text-shadow: none;
	font-family: Arial, Helvetica, sans-serif;
	text-transform: uppercase;
	font-weight: 700;
	width:250px;
	color: #fff;
	padding: 5px 40px;
	margin: 10px 10px;
}
.uk-button-secondary:active, .uk-button-secondary:focus, .uk-button-secondary:hover {
	background-color: #449d44;
	color: #fff
}
.uk-button-primary:active, .uk-button-primary:focus, .uk-button-primary:hover {
	background-color: #3071a9;
	color: #fff;
}

@media only screen and (min-width:768px) and (max-width:959px) {
.container {
	width: 768px
}
}
@media only screen and (max-width:767px) {
.container {
	width: 95%
}

}
@media only screen and (min-width:480px) and (max-width:767px) {
.container {
	width: 70%
}
}
.container:after {
	content: "\0020";
	display: block;
	height: 0;
	clear: both;
	visibility: hidden
}
.uk-navbar-content.uk-navbar-center.uk-visible-small {
	max-width: initial
}
.tm-navbar-small .uk-button-dropdown .uk-button {
	border: 1px solid #000;
	color: #000
}
@media (max-width:767px) {
.uk-navbar-center {
	max-width: 172px
}
.fpull20 {
	padding-left: 20px!important
}
}
@media (max-width:768px) {
.uk-navbar-center {
	max-width: 172px
}
.negmargin, .negmargintop {
	margin-top: 20px!important;
	margin-bottom: 0
}
}
@media(max-width:768px) and (orientation :portrait) {

.tm-fullscreen-container .tm-fullscreen {
	background: url(../resources/images/omni-presence-ipad.jpg) 50% 50%/cover no-repeat #8D8D8D
}
.tm-fullscreen-container .tm-fullscreen.fs2 {
	background: url(../resources/images/omni-presence2-ipad.jpg) 50% 50%/cover no-repeat #8D8D8D;
	bottom: 0
}
.tm-fullscreen-container .tm-fullscreen.fs3 {
	background: url(../resources/images/omni-presence3-ipad.jpg) 50% 50%/cover no-repeat #8D8D8D
}
.bloglist-margin .uk-container {
	padding: 0
}
}
@media (max-width:768px) and (orientation :landscape) {

.tm-fullscreen-container .tm-fullscreen {
	background: url(../resources/images/omni-presence-ipad.jpg) 50% 50%/cover no-repeat #8D8D8D
}
.tm-fullscreen-container .tm-fullscreen.fs2 {
	background: url(../resources/images/omni-presence2-ipad.jpg) 50% 50%/cover no-repeat #8D8D8D;
	bottom: 0
}
.tm-fullscreen-container .tm-fullscreen.fs3 {
	background: url(../resources/images/omni-presence3-ipad.jpg) 50% 50%/cover no-repeat #8D8D8D
}

}


.mainContainer{
  width:100%;
  margin:0 auto;
}