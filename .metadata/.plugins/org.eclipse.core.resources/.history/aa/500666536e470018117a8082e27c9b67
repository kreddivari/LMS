<script>
$(document).ready(function(){
   
$.ajax({
	  url: "current_user",
	  type: "GET",
	  cache: false,
	  success: function(response){
	   
		  var key= $.parseJSON(response);
		  var username=key.data.userName;
		 $("#userName").html(username);
	  }
	});
});
</script>

