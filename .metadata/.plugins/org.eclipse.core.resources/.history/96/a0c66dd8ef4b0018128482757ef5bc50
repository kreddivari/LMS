package com.zen.smi.dao;

import java.io.Serializable;
import java.util.List;
import com.zen.smi.dao.entities.Room;
import com.zen.smi.dao.entities.UserRoom;
import com.zen.smi.dao.exception.GenericDAOException;

public interface UserRoomDAO extends BaseDAO<UserRoom, Serializable> {

	public void addUserRoom(UserRoom userRoom) throws GenericDAOException;
	public void updateRoom(Room room) throws GenericDAOException;	
	public List<Room> getAllRooms() throws GenericDAOException;

}
